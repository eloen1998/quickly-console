{
    "name": "quickly-console",
    "displayName": "Quickly Console",
    "description": "智能console.",
    "publisher": "eloen",
    "author": "eloen",
    "repository": {
        "type": "git",
        "url": "https://github.com/eloen1998/quickly-console"
    },
    "version": "0.5.0",
    "icon": "static/icon.png",
    "engines": {
        "vscode": ">=1.81.0"
    },
    "categories": [
        "Other",
        "Snippets"
    ],
    "main": "./dist/index.js",
    "files": [
        "dist"
    ],
    "activationEvents": [],
    "contributes": {
        "commands": [
            {
                "command": "quicklyConsole.insertConsole",
                "title": "插入打印语句"
            },
            {
                "command": "quicklyConsole.deleteConsole",
                "title": "删除所有打印语句"
            }
        ],
        "keybindings": [
            {
                "command": "quicklyConsole.insertConsole",
                "mac": "cmd+y"
            },
            {
                "command": "quicklyConsole.deleteConsole",
                "mac": "cmd+u"
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets.json"
            },
            {
                "language": "vue",
                "path": "./snippets.json"
            },
            {
                "language": "typescript",
                "path": "./snippets.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "quicklyConsole",
            "properties": {
                "quicklyConsole.singleQuote": {
                    "type": "boolean",
                    "default": true,
                    "description": "使用单引号而不是双引号"
                },
                "quicklyConsole.semi": {
                    "type": "boolean",
                    "default": true,
                    "description": "结尾分号"
                },
                "quicklyConsole.delete.types": {
                    "type": "array",
                    "default": [
                        "log",
                        "time",
                        "timeEnd"
                    ],
                    "description": "需要删除的console类型"
                },
                "quicklyConsole.prefix": {
                    "type": "string",
                    "default": "",
                    "description": "打印语句前缀"
                }
            }
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --external vscode",
        "dev": "nr build --watch",
        "lint": "eslint --ext .ts src/**/*",
        "vscode:prepublish": "nr build",
        "publish": "vsce publish --no-dependencies",
        "pack": "vsce package --no-dependencies",
        "test": "vitest",
        "typecheck": "tsc --noEmit",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "devDependencies": {
        "@babel/parser": "^7.22.10",
        "@babel/traverse": "^7.22.10",
        "@babel/types": "^7.22.10",
        "@types/babel__traverse": "^7.20.1",
        "@types/node": "^20.4.9",
        "@types/vscode": "^1.81.0",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "@vscode/vsce": "^2.20.1",
        "eslint": "^8.46.0",
        "esno": "^0.16.3",
        "tsup": "^6.7.0",
        "typescript": "^5.1.6",
        "vite": "^4.4.9",
        "vitest": "^0.31.4"
    }
}
